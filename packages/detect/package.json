{
  "name": "@sonate/detect",
  "version": "1.4.0",
  "description": "Real-time AI detection and scoring for SONATE platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "node -e \"const fs=require('fs'); try{fs.rmSync('dist',{recursive:true,force:true});fs.rmSync('tsconfig.tsbuildinfo',{force:true})}catch(e){console.error(e);process.exit(1)}\"",
    "test": "npm run build && c8 --check-coverage --branches 55 --functions 55 --lines 60 --statements 60 node dist/tests/run-tests.js",
    "test:adversarial": "jest",
    "test:stakes": "jest src/__tests__/stakes.spec.ts",
    "test:explainable": "jest src/__tests__/explainable.spec.ts",
    "test:stickiness": "jest src/__tests__/stickiness.spec.ts"
  },
  "keywords": [
    "sonate",
    "detection",
    "sonate-framework",
    "ai-monitoring"
  ],
  "author": "YSEEKU",
  "license": "MIT",
  "dependencies": {
    "@sonate/core": "*",
    "axios": "^1.6.0",
    "ioredis": "^5.9.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.1",
    "@types/uuid": "^11.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}

{
  "name": "@sonate/verify-sdk",
  "version": "1.0.0",
  "description": "Client-side SDK for verifying SONATE trust receipts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "sonate",
    "trust",
    "verification",
    "ed25519",
    "receipts",
    "ai-safety"
  ],
  "author": "YCQ Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/s8ken/sonate-verify-sdk"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0"
  }
}

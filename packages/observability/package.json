{
  "name": "@sonate/observability",
  "version": "1.4.0",
  "description": "Enterprise observability and monitoring for SONATE platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "clean": "node -e \"const fs=require('fs'); try{fs.rmSync('dist',{recursive:true,force:true});fs.rmSync('tsconfig.tsbuildinfo',{force:true})}catch(e){console.error(e);process.exit(1)}\"",
    "test": "npm run build && node dist/tests/run-tests.js"
  },
  "keywords": [
    "sonate",
    "observability",
    "opentelemetry",
    "monitoring",
    "metrics",
    "tracing"
  ],
  "author": "YSEEKU",
  "license": "MIT",
  "dependencies": {
    "@sonate/core": "file:../core",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/sdk-node": "^0.51.0",
    "@opentelemetry/sdk-metrics": "^1.22.0",
    "@opentelemetry/sdk-trace-node": "^1.22.0",
    "@opentelemetry/exporter-prometheus": "^0.51.0",
    "@opentelemetry/exporter-jaeger": "^1.22.0",
    "@opentelemetry/exporter-zipkin": "^1.22.0",
    "@opentelemetry/instrumentation-http": "^0.51.0",
    "@opentelemetry/instrumentation-express": "^0.41.0",
    "@opentelemetry/resource-detector-aws": "^1.5.0",
    "@opentelemetry/resource-detector-gcp": "^0.29.0",
    "@opentelemetry/semantic-conventions": "^1.22.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  }
}

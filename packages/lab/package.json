{
  "name": "@sonate/lab",
  "version": "2.0.0",
  "description": "Double-blind experimentation and research for SONATE platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "node -e \"const fs=require('fs'); try{fs.rmSync('dist',{recursive:true,force:true});fs.rmSync('tsconfig.tsbuildinfo',{force:true})}catch(e){console.error(e);process.exit(1)}\"",
    "test": "c8 --check-coverage --branches 30 --functions 25 --lines 30 --statements 30 node src/tests/run-tests.js",
    "test:resonate": "node test-resonate.js",
    "benchmark": "npm run build && npm run test:resonate"
  },
  "keywords": [
    "sonate",
    "experimentation",
    "research",
    "double-blind",
    "a-b-testing"
  ],
  "author": "YSEEKU",
  "license": "MIT",
  "dependencies": {
    "@sonate/core": "*",
    "@sonate/detect": "*"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^25.3.2"
  }
}

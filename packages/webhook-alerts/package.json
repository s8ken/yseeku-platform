{
  "name": "@sonate/webhook-alerts",
  "version": "1.4.0",
  "description": "Real-time webhook alerts for SONATE platform trust violations and events",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "clean": "node -e \"const fs=require('fs'); try{fs.rmSync('dist',{recursive:true,force:true});fs.rmSync('tsconfig.tsbuildinfo',{force:true})}catch(e){console.error(e);process.exit(1)}\"",
    "test": "npm run build && node dist/tests/run-tests.js"
  },
  "keywords": [
    "sonate",
    "webhooks",
    "alerts",
    "notifications",
    "trust-violations",
    "real-time"
  ],
  "author": "YSEEKU",
  "license": "MIT",
  "dependencies": {
    "@sonate/core": "file:../core",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/crypto-js": "^4.2.0",
    "typescript": "^5.0.0"
  }
}

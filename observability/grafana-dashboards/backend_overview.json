{
  "title": "Backend Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests Rate",
      "targets": [{"expr":"sum(rate(http_requests_total[1m])) by (method)"}],
      "datasource": {"type":"prometheus","uid":"Prometheus"}
    },
    {
      "type": "timeseries",
      "title": "HTTP 5xx Rate",
      "targets": [{"expr":"sum(rate(http_requests_total{status_code=~\"5..\"}[1m]))"}],
      "datasource": {"type":"prometheus","uid":"Prometheus"}
    },
    {
      "type": "timeseries",
      "title": "HTTP p95 Duration",
      "targets": [{"expr":"histogram_quantile(0.95, sum by (le) (rate(http_request_duration_seconds_bucket[5m])))"}],
      "datasource": {"type":"prometheus","uid":"Prometheus"}
    },
    {
      "type": "timeseries",
      "title": "DB Query p95",
      "targets": [{"expr":"histogram_quantile(0.95, sum by (le) (rate(db_query_duration_seconds_bucket[5m])))"}],
      "datasource": {"type":"prometheus","uid":"Prometheus"}
    },
    {
      "type": "timeseries",
      "title": "Socket Events Rate",
      "targets": [{"expr":"sum by(event) (rate(socket_events_total[1m]))"}],
      "datasource": {"type":"prometheus","uid":"Prometheus"}
    }
  ]
}

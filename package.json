{
  "name": "yseeku-platform",
  "version": "1.13.0",
  "description": "SONATE Platform - Enterprise AI Trust Framework",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=web",
    "start": "npm start --workspace=web",
    "build:backend": "turbo run build --filter=backend",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test --filter=@sonate/core --filter=@sonate/detect --filter=@sonate/orchestrate --filter=@sonate/lab --filter=@sonate/persistence --filter=@sonate/monitoring",
    "test:integration": "turbo run test --filter=integration-tests",
    "test:e2e": "turbo run e2e --filter=web",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "demo:ledger": "npm run build --workspace @sonate/collaboration-ledger && node examples/ledger-demo/index.js",
    "demo:interactive": "npm run dev --workspace @sonate/new-demo"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.90.0",
    "@types/node": "^20.0.0",
    "c8": "^9.1.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "turbo": "^1.10.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@vitest/ui": "^4.0.16",
    "axios": "^1.13.2",
    "esbuild": "^0.27.2",
    "jsdom": "^27.4.0",
    "lightningcss": "^1.30.2",
    "parse-duration": "^2.1.5",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  },
  "optionalDependencies": {
    "lightningcss-darwin-arm64": "^1.30.2"
  }
}
